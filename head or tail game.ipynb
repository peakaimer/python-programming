    "#Coin game\n",
    "import random\n",
    "my_seed = int(input(\"Enter a seed in number: \"))\n",
    "random.seed(my_seed)\n",
    "random_side = random.randint(0, 1)\n",
    "if random_side == 1:\n",
    "    print(\"Head\")\n",
    "\n",
    "else:\n",
    "    print(\"Tail\")"
